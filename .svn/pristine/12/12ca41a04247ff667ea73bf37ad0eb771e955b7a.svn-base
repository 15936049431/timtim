@charset "utf-8";
/* CSS Document */
.login_bg { height: 766px; background: url(../images/login_bg.jpg) no-repeat center; }
.login_bg .main { position: relative; }

.login_form { width: 310px; padding: 15px 10px; background: #fff; position: absolute; right: 0; top: 150px;animation:gotop 0.3s linear 0s 1 normal;-webkit-animation:gotop 0.3s linear 0s 1 normal;-moz-animation:gotop 0.3s linear 0s 1 normal; }
.login_form dt { height: 46px; line-height: 46px; font-size: 18px; margin-bottom: 25px; padding: 0 15px; border-bottom: 1px solid #cfcfcf; }
.login_form dd { padding: 5px 30px; position: relative; }
.login_form dd em { width: 40px; height: 40px; text-align: center; line-height: 40px; font-size: 16px; color: #a6a6a6; position: absolute; }
.login_form input[type=text],.login_form input[type=password] { display: block; width: 200px; height: 20px; line-height: 20px; font-size: 14px; padding: 10px 10px 10px 44px; border: 1px solid #cfcfcf; border-radius: 5px; }
.login_form p { height: 40px; line-height: 40px; }
.login_form p.tl a { color: #ff8a10; }
.login_form .input_desc { height: 46px; line-height: 46px; left: 70px; }
.login_form .btn { display: block; width: 250px; height: 34px; text-align: center; line-height: 34px; font-size: 16px; color: #fff; background: #ff8a10; border: none; border-radius: 5px; cursor: pointer; }
@keyframes gotop{ from{top:-800px;} to {top:150px;}}
@-webkit-keyframes gotop{ from{top:-800px;} to {top:150px;}}
@-moz-keyframes gotop{ from{top:-800px;} to {top:150px;}}

.reg_main { height: 550px; padding: 75px 0; background: #f2f2f2; }
.reg_form { height: 550px; background: #fff url(../images/reg_bg.png) no-repeat 600px center; position: relative;}
.reg_form:after { display: block; content: "";/* width: 202px; height: 264px;*/ background: url(../images/shadow.png) no-repeat; position: absolute; right: -202px; top: 0; }
.reg_form table { width: 358px; margin-left: 78px; }
.reg_form th { line-height: 95px; font-size: 18px; color: #0562a4; }
.reg_form td { padding-bottom: 10px; position: relative; }
.reg_form input[type=text],.reg_form input[type=password] { width: 302px; height: 20px; line-height: 20px; padding: 10px 10px 10px 44px; background: #f5f5f5 no-repeat; border-radius: 3px; }
.reg_form input[type=text]:focus,.reg_form input[type=password]:focus { border-color: #ff8a10; }
.reg_form input.uname { background-position: 14px center; background-image: url(data:image/png;base64,); }
.reg_form input.phone { background-position: 14px center; background-image: url(data:image/png;base64,); }
.reg_form input.code { width: 154px; background-position: 14px center; background-image: url(data:image/png;base64,); }
.reg_form input.pwd { background-position: 14px center; background-image: url(data:image/png;base64,); }
.reg_form .send {border:none;display: block; float: right; width: 130px; height: 42px; text-align: center; line-height: 42px; font-size: 14px; color: #fff; background: #ff8a10; border-radius: 3px; }
.reg_form span.input_desc { line-height: 42px; left: 45px; }
.reg_form .Validform_checktip { display: none; line-height: 42px; font-size: 14px; color: #fff; background: #9fd2f5; padding: 0 10px; border-radius: 3px; position: absolute; left: 368px; top: 0; white-space: nowrap; }
.reg_form .Validform_checktip:after { content: ""; border-top: 5px solid transparent; border-right: 7px solid #9fd2f5; border-bottom: 5px solid transparent; position: absolute; left: -7px; top: 16px; }
.reg_form .Validform_checktip.Validform_wrong { display: block !important; }
.reg_form label { display: inline-block; vertical-align: middle; margin: 10px 0; }
.reg_form label a { text-decoration: underline; color: #ff8a10; margin: 0 5px; }
.reg_form .btn { display: block; width: 100%; height: 48px; text-align: center; line-height: 48px; font-size: 18px; color: #fff; background: #ff8a10; border: none; border-radius: 3px; cursor: pointer; }
.reg_form .tl { height: 44px; font-size: 14px; padding-top: 40px; }
.reg_form .tl a { font-size: 14px; color: #6da6e8; }