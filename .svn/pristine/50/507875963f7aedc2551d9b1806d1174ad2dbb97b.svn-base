@charset "utf-8";
.inv_require { padding: 0 15px; background: #fff; }
.inv_require h1 { height: 50px; line-height: 50px; font-size: 18px; color: #000; }
.inv_require dl { padding: 5px 0 15px 0; }
.inv_require dt { float: left; height: 25px; line-height: 25px; font-size: 16px; }
.inv_require dd { float: left; }
.inv_require dd a { display: block; height: 25px; line-height: 24px; font-size: 14px; padding: 0 15px; border-radius: 5px; }
.inv_require dd.sel a { color: #fff; background: #ff8a10; }

.inv_tenders { background: #fff; }
.inv_tenders table { width: 100%; }
.inv_tenders th { height: 40px; font-size: 14px; background: #f4f4f4; border-bottom: 1px solid #cfcfcf; }
.inv_tenders td { height: 100px; text-align: center; font-size: 18px; color: #000; background: #fff; border-bottom: 1px solid #cfcfcf; }
.inv_tenders tr td:first-child { text-align: left; padding-left: 15px; }
.inv_tenders a.tit { max-width: 202px; display: block; font-size: 18px; color: #000; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; }
.inv_tenders p { height: 20px; line-height: 20px; font-size: 14px; padding-left: 20px; background: no-repeat left center url(data:image/png;base64,); }
.inv_tenders i { font-size: 30px; color: #ff8a10; }
.inv_tenders em { font-size: 28px; }
.inv_tenders s { text-decoration: none; color: #bfbfbf; }
.inv_tenders .progress_c { width: 60px; height: 60px; margin: 0 auto; position: relative; }
.inv_tenders .progress_c span { width: 60px; height: 60px; text-align: center; line-height: 60px; font-size: 16px; color: #ff8a10; position: absolute; left: 0; top: 0; }
.inv_tenders .progress_c div { overflow: visible !important; }
.inv_tenders a.btn,.inv_tenders span.btn { display: block; width: 110px; height: 36px; text-align: center; line-height: 36px; font-size: 18px; border-radius: 5px; }
.inv_tenders a.btn { color: #fff; background: #ff8a10; }
.inv_tenders span.btn  { color: #7a7a7a; background: #d4d4d4; }

.inv_c_info { padding: 0 30px 10px 30px; background: #fff; }
.inv_c_info_l { width: 50%; padding: 20px 0; }
.inv_c_info_l h1 { height: 55px; line-height: 55px; font-size: 18px; }
.inv_c_info_l dl { float: left; width: 183px; text-align: center; line-height: 30px; padding: 10px 0; }
.inv_c_info_l dl:first-child { width: 153px; text-align: left; }
.inv_c_info_l dl dt { font-size: 14px; color: #7a7a7a; }
.inv_c_info_l dl dd { font-size: 24px; color: #000; }
.inv_c_info_l ul { margin-top: 30px; }
.inv_c_info_l ul li { float: left; width: 50%; height: 35px; line-height: 35px; }
.inv_c_info_l ul span { float: left; width: 70px; font-size: 14px; color: #7a7a7a; }
.inv_c_info_l ul i { float: left; font-size: 14px; color: #000; }
.inv_c_info_l ul a { font-size: 14px; }
.inv_c_info_l ul i.end_time { color: #7a7a7a; }
.inv_c_info_r { width: 50%; position: relative; }
.inv_c_info_r dl { width: 210px; height: 100%; text-align: center; border-left: 1px solid #cfcfcf; padding-top: 10px; position: absolute; left: 0; top: 0; }
.inv_c_info_r dt { height: 55px; line-height: 55px; font-size: 18px; color: #000; }
.inv_c_info_r .progress_c_big { width: 96px; height: 96px; margin: 0 auto; position: relative; }
.inv_c_info_r .progress_c_big span { width: 96px; height: 96px; text-align: center; line-height: 96px; font-size: 20px; color: #ff8a10; position: absolute; left: 0; top: 0; }
.inv_c_info_r .progress_c_big div { overflow: visible !important; }
.inv_c_info_r dd { padding-top: 25px; }
.inv_c_info_r dd h4 { font-size: 14px; color: #7a7a7a; }
.inv_c_info_r dd p { font-size: 20px; color: #ff8a10; }
.inv_c_info_r ul { width: 270px; padding-top: 25px; }
.inv_c_info_r ul li { font-size: 14px; color: #000; padding-bottom: 10px; position: relative; }
.inv_c_info_r ul input[type=text] { width: 118px; height: 20px; font-size: 14px; padding: 12px 100px 12px 50px; border: 1px solid #cfcfcf; }
.inv_c_info_r ul input[type=password] { width: 248px; height: 20px; font-size: 14px; padding: 12px 10px; border: 1px solid #cfcfcf; }
.inv_c_info_r ul input.authcode{width:120px;padding:12px 10px 12px 10px;}
.inv_c_info_r ul .btn {cursor:pointer; display: block; width: 100%; height: 36px; text-align: center; line-height: 36px; font-size: 18px; color: #fff; background: #ff8a10; border: none; }
.inv_c_info_r ul li span { width: 50px; height: 44px; text-align: center; line-height: 44px; font-size: 22px; position: absolute; left: 0; top: 0; }
.inv_c_info_r ul li a { width: 80px; height: 24px; text-align: center; line-height: 24px; font-size: 14px; color: #fff; background: #ff8a10; border-radius: 5px; position: absolute; right: 10px; top: 10px; }
.inv_c_info_r .over_login { display: none; width: 548px; height:100%;min-height:285px; opacity:0.9;background:#fff url(../images/over_login.png) no-repeat center; position: absolute; left: 1px; top: 0; animation:gothis 0.3s linear 0s 1 normal;-webkit-animation:gothis 0.3s linear 0s 1 normal;-moz-animation:gothis 0.3s linear 0s 1 normal; }
.inv_c_info_r.over_login .over_login { display: block; }
.inv_c_info_r .over_login .over_login_login{display:block;width:200px;height:60px;position:relative;top:120px;margin:auto;}
.inv_c_info_r .over_full { display: none; width: 548px; height:100%;min-height:252px; background: rgba(255, 255, 255, .9) url(../images/over_full.png) no-repeat center; position: absolute; left: 1px; top: 0; animation:gothis 0.3s linear 0s 1 normal;-webkit-animation:gothis 0.3s linear 0s 1 normal;-moz-animation:gothis 0.3s linear 0s 1 normal;}
.inv_c_info_r.over_full .over_full { display: block; }
.inv_c_info_r .over_first { display: none; width: 548px; height:100%;min-height:252px; background: rgba(255, 255, 255, .9) url(../images/over_first.png) no-repeat center; position: absolute; left: 1px; top: 0; animation:gothis 0.3s linear 0s 1 normal;-webkit-animation:gothis 0.3s linear 0s 1 normal;-moz-animation:gothis 0.3s linear 0s 1 normal;}
.inv_c_info_r.over_first .over_first { display: block; }
.inv_c_info_r .over_repay { display: none; width: 548px; height:100%;min-height:252px; background: rgba(255, 255, 255, .9) url(../images/over_repay.png) no-repeat center; position: absolute; left: 1px; top: 0; animation:gothis 0.3s linear 0s 1 normal;-webkit-animation:gothis 0.3s linear 0s 1 normal;-moz-animation:gothis 0.3s linear 0s 1 normal;}
.inv_c_info_r.over_repay .over_repay { display: block; }
.inv_c_info_r .over_fail { display: none; width: 548px; height:100%;min-height:252px; background: rgba(255, 255, 255, .9) url(../images/over_fail.png) no-repeat center; position: absolute; left: 1px; top: 0; animation:gothis 0.3s linear 0s 1 normal;-webkit-animation:gothis 0.3s linear 0s 1 normal;-moz-animation:gothis 0.3s linear 0s 1 normal;}
.inv_c_info_r.over_fail .over_fail { display: block; }
.inv_c_info_r .over_liu { display: none; width: 548px; height:100%;min-height:252px; background: rgba(255, 255, 255, .9) url(../images/over_liu.png) no-repeat center; position: absolute; left: 1px; top: 0; animation:gothis 0.3s linear 0s 1 normal;-webkit-animation:gothis 0.3s linear 0s 1 normal;-moz-animation:gothis 0.3s linear 0s 1 normal;}
.inv_c_info_r.over_liu .over_liu { display: block; }
.inv_c_info_r .over_cancle { display: none; width: 548px; height:100%;min-height:252px; background: rgba(255, 255, 255, .9) url(../images/over_cancle.png) no-repeat center; position: absolute; left: 1px; top: 0; animation:gothis 0.3s linear 0s 1 normal;-webkit-animation:gothis 0.3s linear 0s 1 normal;-moz-animation:gothis 0.3s linear 0s 1 normal;}
.inv_c_info_r.over_cancle .over_cancle { display: block; }
.inv_c_info_r .over_ending { display: none; width: 548px; height:100%;min-height:252px; background: rgba(255, 255, 255, .9) url(../images/over_ending.png) no-repeat center; position: absolute; left: 1px; top: 0; animation:gothis 0.3s linear 0s 1 normal;-webkit-animation:gothis 0.3s linear 0s 1 normal;-moz-animation:gothis 0.3s linear 0s 1 normal;}
.inv_c_info_r.over_ending .over_ending { display: block; }
@keyframes gothis{ from{transform:rotate(180deg)} to{transform:rotate(0deg)}}
@-wekbit-keyframes gothis{ from{transform:rotate(180deg)} to{transform:rotate(0deg)}}
@-moz-keyframes gothis{ from{transform:rotate(180deg)} to{transform:rotate(0deg)}}
.inv_c_content { background: #fff; }
.inv_c_tab li {cursor:pointer; float: left; border-right: 1px solid #cfcfcf; }
.inv_c_tab li span { display: block; width: 200px; height: 56px; text-align: center; line-height: 56px; font-size: 18px; color: #000; padding: 5px 0; background: #f6f6f6; }
.inv_c_tab li.sel span { padding-top: 0; background: #fff; border-top: 5px solid #ff8a10; }
.inv_c_content_in { padding: 30px; }
.inv_c_info_list { line-height: 30px; font-size: 14px; padding: 10px 0 0 50px; }
.inv_c_record { width: 100%; text-align: center; font-size: 14px; }
.inv_c_record td { height: 40px; }
.inv_c_record tr:nth-child(2n+1) td { background: #f6f6f6; }