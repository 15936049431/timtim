@charset "utf-8";
/* CSS Document */
.user_main { min-height: 364px; position: relative; }
.user_menu { width: 220px; background: #fff; }
.user_menu li > a,.user_menu i { display: block; height: 50px; border-bottom: 2px solid #f4f4f4; cursor: pointer; }
.user_menu li > a em,.user_menu i em { display: table-cell; vertical-align: middle; width: 70px; height: 50px; background: url(../images/user_menu.png) no-repeat; }
.user_menu .m1 em { background-position: right 0px; }
.user_menu .m2 em { background-position: right -100px; }
.user_menu .m3 em { background-position: right -200px; }
.user_menu .m4 em { background-position: right -300px; }
.user_menu .m5 em { background-position: right -400px; }
.user_menu .m6 em { background-position: right -500px; }
.user_menu .m7 em { background-position: right -600px; }
.user_menu li > a span,.user_menu i span { display: table-cell; vertical-align: middle; width: 135px; height: 50px; font-size: 18px; color: #828282; padding-left: 10px; }
.user_menu li > a b{width:5px;display:table-cell}
.user_menu li:hover span,.user_menu .sel span { color: #ff8a10; }
.user_menu .m1:hover em,.user_menu .m1.sel em { background-position: right -50px; }
.user_menu .m1:hover b,.user_menu .m1.sel b{height:100%;background:#ff8a10;}
.user_menu .m2:hover em,.user_menu .m2.sel em { background-position: right -150px; }
.user_menu .m2:hover b,.user_menu .m1.sel b{height:100%;background:#ff8a10;}
.user_menu .m3:hover em,.user_menu .m3.sel em { background-position: right -250px; }
.user_menu .m3:hover b,.user_menu .m1.sel b{height:100%;background:#ff8a10;}
.user_menu .m4:hover em,.user_menu .m4.sel em { background-position: right -350px; }
.user_menu .m4:hover b,.user_menu .m1.sel b{height:100%;background:#ff8a10;}
.user_menu .m5:hover em,.user_menu .m5.sel em { background-position: right -450px; }
.user_menu .m5:hover b,.user_menu .m1.sel b{height:100%;background:#ff8a10;}
.user_menu .m6:hover em,.user_menu .m6.sel em { background-position: right -550px; }
.user_menu .m6:hover b,.user_menu .m1.sel b{height:100%;background:#ff8a10;}
.user_menu .m7:hover em,.user_menu .m7.sel em { background-position: right -650px; }
.user_menu .m7:hover b,.user_menu .m1.sel b{height:100%;background:#ff8a10;}
.user_menu dl { display: none; border-bottom: 2px solid #f4f4f4; }
.user_menu dl a { display: block; height: 30px; line-height: 30px; font-size: 16px; padding-left: 75px; }
.user_menu dl a:hover,.user_menu dl dd.sel a { color: #ff8a10; }
.user_con { width: 860px }
.user_info { padding: 20px 20px 0 20px; background: #fff; }
.user_info dl dt { height: 45px; line-height: 45px; font-size: 14px; color: #acacac; margin-top: 10px; padding-left: 25px; background: no-repeat left center url(data:image/png;base64,); }
.user_avatar { width: 110px; height: 110px; }
.user_avatar img { width: 110px; height: 110px; }
.user_info_column1 { width: 200px; padding-left: 20px; }
.user_info_column1 h2 { height: 24px; font-size: 18px; color: #acacac; }
.user_info_column1 h1 { height: 36px; font-size: 28px; color: #838383; }
.user_progress { width: 160px; height: 20px; margin: 4px 0; border: 1px solid #8cc01d; border-radius: 11px; overflow: hidden; }
.user_progress em { display: block; width: 0; height: 20px; background: #8cc01d; }
.user_progress .low { width: 33%; }
.user_progress .mid { width: 66%; }
.user_progress .hgt { width: 100%; }
.user_info_column1 p { height: 20px; line-height: 20px; font-weight: 14px; color: #acacac; }
.user_info_column2 { width: 330px; color: #838383; padding: 0px 0 24px 20px; }
.user_info_column2 h1 { height: 36px; font-size: 20px; }
.user_info_column2 h1 i { font-size: 28px; color: #ff8a10; }
.user_info_column2 h3 { height: 20px; font-size: 14px; margin-top:6px; }
.user_main_btns a { display: block; width: 138px; height: 46px; text-align: center; line-height: 46px; font-size: 20px; margin-bottom: 15px; border: 1px solid; border-radius: 5px; }
.user_main_btns a.btn1 { color: #fff; background: #ff8a10; border-color: #ff8a10; }
.user_main_btns a.btn2 { color: #acacac; border-color: #acacac; }

.user_column { padding: 0 20px; background: #fff; }
.user_column_left { width: 490px; }
.user_column_left h1 { height: 60px; line-height: 60px; }
.user_column_left h1 i { font-size: 14px; color: #acacac; margin-left: 20px; }
.user_column_left dd { float: left; width: 240px; height: 70px; text-align: center; background: #f4f4f4; }
.user_column_left dd h3 { height: 40px; line-height: 40px; font-size: 16px; color: #acacac; }
.user_column_left dd p { font-size: 20px; }
.user_column_left .user_day_total { height: 70px; line-height: 70px; font-size: 16px; }
.user_column_left .user_day_total i { font-size: 24px; color: #ff8a10; }
.user_column_right { width: 310px; height: 350px; padding: 15px 0; background: #f4f4f4; }
.user_column_right li { height: 30px; line-height: 30px; text-align: center; font-size: 0; }
.user_column_right li span { display: inline-block; width: 174px; font-size: 14px; }
.user_column_right li i { display: inline-block; width: 136px; font-size: 14px; }

.user_main_tools { height: 50px; line-height: 50px; font-size: 0; color: #acacac; padding-top: 20px; }
.user_main_tools span { display: inline-block; height: 50px; font-size: 14px; }
.user_main_tools a { display: inline-block; height: 50px; font-size: 14px; color: #acacac; }
.user_main_tools .qq { display: inline-block; width: 260px; height: 50px; font-size: 0; margin-right: 120px; }
.user_main_tools .qq a { padding-left: 30px; background: no-repeat left center url(data:image/png;base64,); }
.user_main_tools .auto { display: inline-block; width: 160px; height: 50px; font-size: 0; margin-right: 30px; padding-left: 30px; background: no-repeat left center url(data:image/png;base64,); }
.user_main_tools .zq { width: 60px; margin-right: 30px; padding-left: 35px; background: no-repeat left center url(data:image/png;base64,); }
.user_main_tools .fri { width: 65px; padding-left: 30px; background: no-repeat left center url(data:image/png;base64,); }

.user_money_main { padding: 30px; background: #fff; min-height:500px;}
.user_money_tab { height: 50px; border-bottom: 2px solid #cfcfcf; }
.user_money_tab dd { float: left; }
.user_money_tab dd a { display: block; width: 130px; height: 50px; text-align: center; line-height: 50px; font-size: 18px; }
.user_money_tab dd.sel a { color: #ff8a10; border-bottom: 2px solid #ff8a10; }
.user_money_main table { width: 100%; }
.user_money_main table td { min-height: 50px; line-height: 50px; font-size: 16px; padding: 10px 0; }
.user_money_main table tr td:first-child { vertical-align: top; width: 120px; text-align: right; }
.user_money_main table .recharge_type dd { float: left; width: 130px; height: 36px; text-align: center; line-height: 36px; font-size: 14px; margin-right: 20px; border: 1px solid #cfcfcf; cursor: pointer; }
.user_money_main table .recharge_type dd.sel { background: #feecd6; border-color: #ff8a10; position: relative; }
.user_money_main table .recharge_type dd.sel:after { content: ""; width: 22px; height: 20px; position: absolute; right: 0; bottom: 0; background: url(data:image/png;base64,); }
.user_money_main table .recharge_choose { max-width: 676px; }
.user_money_main table .recharge_choose li { float: left; min-width: 168px; height: 50px; border-right: 1px solid #cfcfcf; border-bottom: 1px solid #cfcfcf; position: relative; cursor: pointer; }
.user_money_main table .recharge_choose li em { min-width: 168px; height: 50px; border: 1px solid transparent; position: absolute; left: -1px; top: -1px; }
.user_money_main table .recharge_choose li span { display: table-cell; vertical-align: middle; min-width: 168px; height: 50px; text-align: center; line-height: 25px; }
.user_money_main table .recharge_choose li span img { vertical-align: middle; }
.user_money_main table .recharge_choose li.sel em { border-color: #ff8a10; }
.user_money_main table .recharge_choose li.sel em:after { content: ""; width: 22px; height: 20px; position: absolute; right: 0; bottom: 0; background: url(data:image/png;base64,); }
.user_money_main table .recharge_choose li.overline{float:left;width:300px;height:100px;border-right: 1px solid #cfcfcf;border-bottom:1px solid #cfcfcf;position:relative;cursor:pointer;}
.user_money_main table .recharge_choose li.overline em { width: 300px; height: 100px; border: 1px solid transparent; position: absolute; left: -1px; top: -1px; }
.user_money_main table .recharge_choose li.overline span { display: table-cell; vertical-align: middle; width: 300px; height: 100px; text-align: center; line-height: 25px; }
.user_money_main table .recharge_choose li.overline span img { vertical-align: middle; }
.user_money_main table .recharge_choose li.overline.sel em { border-color: #ff8a10; }
.user_money_main table span.i { margin: 0 8px; }
.user_money_main table i { color: #ff8a10; }
.user_money_main table input[type=text],.user_money_main table input[type=password] { width: 200px; height: 20px; line-height: 20px; font-size: 14px; padding: 10px; border: 1px solid #cfcfcf; }
.user_money_main table input[type=text].code { width: 90px; }
.user_money_main table img.code { vertical-align: middle; margin-left: 10px; }
.user_money_main table input[type=submit].btn { width: 150px; height: 45px; text-align: center; line-height: 45px; font-size: 16px; color: #fff; background: #ff8a10; border-radius: 5px; border: none; }
.user_money_main table input[type=button].btn { cursor:pointer; width: 150px; height: 45px; text-align: center; line-height: 45px; font-size: 16px; color: #fff; background: #ff8a10; border-radius: 5px; border: none; }
.user_mone_desc { width: 630px; line-height: 25px; font-size: 14px; }
.user_mone_desc i { color: #ff8a10; }

.user_tit { height: 50px; line-height: 50px; border-bottom: 2px solid #cfcfcf; }
.user_tit span { display: block; width: 150px; height: 50px; text-align: center; font-size: 18px; color: #ff8a10; border-bottom: 2px solid #ff8a10; }

.user_cark_list { min-height: 364px; margin-top: 30px; }
.user_cark_list dd { float: left; width: 270px; height: 120px; margin: 0 30px 30px 30px; padding: 15px; border: 1px solid #cfcfcf; border-radius: 10px; position: relative; overflow: hidden; cursor: pointer; }
.user_cark_list dd:hover { border-color: #ff8a10; }
.user_cark_list dd.sel { border-color: #ff8a10; }
.user_cark_list dd.sel:after { content: ""; width: 22px; height: 20px; position: absolute; right: 0; bottom: 0; background: url(data:image/png;base64,); }
.user_cark_list .bank_logo { float: left; height: 40px; }
.user_cark_list .bank_logo em { display: table-cell; vertical-align: middle; height: 40px; line-height: 40px; }
.user_cark_list .bank_logo img { vertical-align: middle; max-height: 40px; }
.user_cark_list .unionpay { float: right; width: 66px; height: 40px; background: url(../images/unionpay.png); }
.user_cark_list .card_info { position: absolute; bottom: 15px; }
.user_cark_list .card_info h3 { height: 26px; line-height: 26px; font-size: 16px; }
.user_cark_list .card_info p { height: 30px; line-height: 30px; font-size: 24px; }
.user_cark_list dt { float: left; width: 270px; height: 120px; margin: 0 30px 30px 30px; padding: 15px; border: 1px dashed #cfcfcf; border-radius: 10px; position: relative; cursor: pointer; }
.user_cark_list dt em { width: 100%; text-align: center; font-size: 14px; color: #aaa; position: absolute; left: 0; top: 90px; }
.user_cark_list dt em:before { content: ""; width: 4px; height: 28px; background: #aaa; position: absolute; left: 148px; top: -50px; }
.user_cark_list dt em:after { content: ""; width: 28px; height: 4px; background: #aaa; position: absolute; left: 136px; top: -38px; }
.user_cark_list dt:hover { border-color: #ff8a10; }
.user_cark_list dt:hover em { color: #ff8a10; }
.user_cark_list dt:hover em:before { background: #ff8a10; }
.user_cark_list dt:hover em:after { background: #ff8a10; }

.user_binding { width: 350px; font-size: 14px; margin: 20px auto 0 auto; }
.user_binding td { padding: 10px 0; }
.user_binding input[type=text] { height: 20px; padding: 2px 10px; border: 1px solid #cfcfcf; }
.user_binding input[type=text]:focus { border-color: #ff8a10; }
.user_binding input[type=text].short { width: 100px; }
.user_binding input[type=text].long { width: 200px; }
.user_binding select { width: 200px; height: 26px; padding-left: 10px; border: 1px solid #cfcfcf; }
.user_binding .tc {text-align:center;}
.user_binding .btn { width: 100px; height: 35px; text-align: center; line-height: 35px; font-size: 16px; color: #fff; margin: 0 15px; background: #ff8a10; border: none; border-radius: 5px; cursor: pointer; }
.user_binding .btn_g { width: 100px; height: 35px; text-align: center; line-height: 35px; font-size: 16px; color: #fff; margin: 0 15px; background: #d4d4d4; border: none; border-radius: 5px; cursor: pointer; }

.user_search { height: 80px; line-height: 80px; font-size: 0; }
.user_search span { display: inline-block; vertical-align: middle; width: 80px; text-align: center; font-size: 16px; }
.user_search select { display: inline-block; vertical-align: middle; width: 160px; height: 30px; font-size: 14px; border: 1px solid #cfcfcf; }
.user_search input[type=text] { display: inline-block; vertical-align: middle; width: 138px; height: 20px; font-size: 14px; padding: 4px 10px; border: 1px solid #cfcfcf; }
.user_search i { display: inline-block; vertical-align: middle; width: 40px; text-align: center; font-size: 16px; color: #979797; }
.user_search .btn { display: inline-block; vertical-align: middle; width: 70px; height: 30px; text-align: center; line-height: 30px; font-size: 16px; color: #fff; margin-left: 20px; background: #ff8a10; border: none; cursor: pointer; }

.user_money_main table.user_recharge_record { width: 100%; /*box-shadow: inset 1px 1px 3px rgba(0, 0, 0, .1); */}
.user_money_main table.user_recharge_record th { height: 35px; font-size: 16px; border-bottom: 1px solid #ff8a10; }
.user_money_main table.user_recharge_record td:first-child{text-align:center;}
.user_money_main table.user_recharge_record td { height: 45px; text-align: center; font-size: 14px; color: #979797;padding:0; }
.user_money_main table.user_recharge_record td i { color: #ff8a10; }

.user_safety { padding: 20px 20px 0 20px; background: #fff; }
.user_safety dl dt { height: 50px; text-align: center; line-height: 50px; }
.user_safety dl dt a { font-size: 14px; color: #acacac; }
.user_safety dl dt a:hover { color: #ff8a10; }
.user_safety_info { margin-left: 30px; font-size: 14px; }
.user_safety_info li { padding-bottom: 15px; }
.user_safety_info li a.btn_a { float: left; width: 134px; height: 36px; text-align: center; line-height: 36px; font-size: 14px; color: #fff; background: #ff8a10; border: 1px solid #ff8a10; border-radius: 5px; }
.user_safety_info li a.btn_b { float: left; width: 134px; height: 36px; text-align: center; line-height: 36px; font-size: 14px; color: #afafaf; margin-left: 20px; border: 1px solid #afafaf; border-radius: 5px; }
.user_progress_s { display: inline-block; vertical-align: middle; width: 330px; height: 20px; margin: 4px 0; border: 1px solid #8cc01d; border-radius: 11px; }
.user_progress_s em { display: block; width: 0; height: 20px; background: #8cc01d; border-radius: 11px; }
.user_progress_s .low { width: 33%; }
.user_progress_s .mid { width: 66%; }
.user_progress_s .hgt { width: 100%; }
.user_safety_list { background: #fff; }
.user_safety_list h1 { height: 60px; line-height: 60px; padding: 0 30px; border-bottom: 1px solid #cfcfcf; }
.user_safety_list li { height: 75px; line-height: 75px; padding: 0 50px 0 90px; border-bottom: 1px solid #cfcfcf; background: no-repeat 55px center url(data:image/png;base64,); }
.user_safety_list li span { float: left; width: 230px; font-size: 18px; }
.user_safety_list li em { float: left; width: 330px; text-align: center; font-size: 16px; color: #c2c2c2; }
.user_safety_list li a { float: right; font-size: 16px; color: #ff8a10; }
.user_safety_list li.ok { background-image: url(data:image/png;base64,); }
.user_safety_list p.tips { height: 60px; line-height: 60px; font-size: 14px; padding: 0 50px; }

.user_acount_main { padding: 30px; background: #fff; min-height:500px;}
.user_acount_list .fl,.user_acount_list .fr { width: 360px; margin: 20px; }
.user_acount_list dl { height: 350px; }
.user_acount_list dt { height: 40px; line-height: 40px; font-size: 16px; margin-bottom: 15px; padding: 0 25px; border: 1px solid; position: relative; }
.user_acount_list .fl dt { background: #ffe8cf; border-color: #ff8a10; }
.user_acount_list .fr dt { background: #e8f2d2; border-color: #8cc01d; }
.user_acount_list dt i { font-size: 16px; position: absolute; right: 25px; top: 0; }
.user_acount_list .fl dt i { color: #ff8a10; }
.user_acount_list .fr dt i { color: #8cc01d; }
.user_acount_list dd { height: 40px; line-height: 40px; font-size: 14px; padding: 0 25px; border-bottom: 1px solid #dfdfdf; }
.user_acount_list dd span { float: left; width: 150px; text-align: right; }
.user_acount_list dd span em { display: inline-block; vertical-align: middle; width: 16px; height: 16px; margin-left: 5px; cursor: pointer; background: url(data:image/png;base64,); }
.user_acount_list dd i { float: right; }
.user_acount_list dd.s { font-size: 12px; }
.user_acount_list .earning_pay { height: 40px; text-align: right; line-height: 40px; font-size: 14px; padding-right: 25px; background: #dfdfdf; }
.user_acount_list .fl .earning_pay { color: #ff8a10; }
.user_acount_list .fr .earning_pay { color: #8cc01d; }
.user_acount_list .earning_pay span { float: left; width: 80px; text-align: center; color: #fff; position: relative; }
.user_acount_list .earning_pay span:after { content: ""; border-top: 7px solid transparent; border-left: 14px solid; border-bottom: 7px solid transparent; position: absolute; right: -14px; top: 13px; }
.user_acount_list .fl .earning_pay span { background: #ff8a10; }
.user_acount_list .fr .earning_pay span { background: #8cc01d; }
.user_acount_list .fl .earning_pay span:after { border-left-color: #ff8a10; }
.user_acount_list .fr .earning_pay span:after { border-left-color: #8cc01d; }
.user_acount_table { width: 100%; font-size: 14px; }
.user_acount_table th { height: 40px; background: #dfdfdf; }
.user_acount_table td { height: 40px; text-align: center; border-bottom: 1px solid #dfdfdf; }